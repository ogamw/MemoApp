{
  "expo": {
    "name": "MemoApp",
    "slug": "MemoApp",
    "version": "1.0.0",
    "privacy": "unlisted",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "jp.suijoji.memoapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "[テスト文言]収集したデータは、他社ウェブサイトで表示する広告の改善に使われます。"
      }
    },
    "android": {
      "package": "jp.suijoji.memoapp",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_adaptive_foreground.png",
        "backgroundImage": "./assets/icon_adaptive_background.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-tracking-transparency"
]
  }
}
